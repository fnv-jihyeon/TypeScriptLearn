{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 127.0.0.1 --port 5173 --strictPort",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",

    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",

    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test \"npm run dev:all\" http://127.0.0.1:5173 \"cypress run\""
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "naive-ui": "^2.42.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^15.0.0",
    "jsdom": "^26.1.0",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
